<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:void="http://rdfs.org/ns/void#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#"  xmlns:vann="http://purl.org/vocab/vann/" xmlns:skos="http://www.w3.org/2004/02/skos/core#"  xmlns:eop="http://www.opengis.net/ont/eo-geojson/1.0/"   xmlns:media="http://search.yahoo.com/mrss/" xmlns:iana="http://www.iana.org/assignments/relation/"  xmlns:owc="http://www.opengis.net/ont/owc/1.0/">

	<owl:Ontology rdf:about="&eop;">
		<dc:title>OGC 17-003 - Earth Observation Dataset Metadata Vocabulary</dc:title>
		<owl:versionInfo rdf:datatype="&xsd;string">1.0.0</owl:versionInfo>
		<void:vocabulary rdf:resource="&dcterms;"/>
		<!-- annotations for documentation tools -->
		<dc:date rdf:datatype="&xsd;string">2018-11-09</dc:date>
		<rdfs:comment rdf:datatype="&xsd;string">This vocabulary defines the classes and properties required to model Earth Observation (EO) metadata for datasets (i.e. products or granules).  It provides the models for the exchange of information describing EO datasets, both within and between different organisations.
        The implementation is derived from the conceptual models defined in the Earth Observation Metadata Profile of Observations and Measurements OGC 10-157r4, and the Unified Metadata Model for Granules (UMM-G).</rdfs:comment>
		
		<vann:preferredNamespacePrefix>eop</vann:preferredNamespacePrefix>
		<vann:preferredNamespaceUri>&eop;</vann:preferredNamespaceUri>
		
		<dc:creator rdf:datatype="&xsd;string">Yves Coene, Spacebel s.a.</dc:creator>
		<dc:contributor rdf:datatype="&xsd;string">John Taylor, CGI</dc:contributor>
		<dc:contributor rdf:datatype="&xsd;string">Olivier Barois, European Space Agency</dc:contributor>
        <dc:contributor rdf:datatype="&xsd;string">Sirko Schindler, DLR</dc:contributor>
        <dcterms:license>http://www.opengeospatial.org/ogc/software</dcterms:license>
		<!-- <owl:imports rdf:resource="&dcterms;"/> -->
		<!-- <owl:imports rdf:resource="&ical;"/> -->	
		
        <!--       <owl:imports rdf:resource="&owc;"/>   -->
        <owl:imports rdf:resource="http://geo.spacebel.be/opensearch/owc.owl"/>  <!-- temporary import location for owc vocabulary  -->
		<owl:imports rdf:resource="&gj;"/>   
  
	</owl:Ontology>

<!-- 
 
-->


<!-- 
       Table 4: EarthObservation Class and properties 
-->

<owl:Class rdf:about="&eop;EarthObservation">
  <rdfs:label>EarthObservation</rdfs:label>
  <rdfs:comment> Earth Observation as defined in OGC 10-157r4</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
  <rdfs:subClassOf rdf:resource="&gj;Feature"/> 
  <vann:example>
{
    "@context": "https://www.opengis.net/eo-geojson/1.0",
    "type": "Feature",
    "id": http://fedeo.esa.int/opensearch/request/?parentIdentifier=SEA_GEC_1P&amp;uid=SE1_OPER_SEA_GEC_1P_19780927T010430_19780927T010445_001316_0000_2267_9B4F",
    "bbox": [
        -2.69574,
        61.965195,
        0.135472,
        63.261372
    ],
    "geometry": {...},
    "properties": {...}
}
  </vann:example>
</owl:Class>


<!-- 
       Table 5: Properties Class and properties 
-->

<owl:Class rdf:about="&eop;Properties">
  <rdfs:label>Properties</rdfs:label>
  <rdfs:comment>Object containing the properties of the Earth Observation.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
  <rdfs:subClassOf rdf:resource="&eop;DataIdentification"/>
  <rdfs:subClassOf rdf:resource="&owc;Properties"/>   <!-- NEW FOR SPLIT -->
  <rdfs:seeAlso rdf:resource="https://tools.ietf.org/html/rfc7946"/>
</owl:Class>

<owl:Class rdf:about="&eop;StatusType">
  <rdfs:label>StatusType</rdfs:label>
  <rdfs:comment>Enumeration of acquisition status constants.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
</owl:Class>

<eop:StatusType rdf:about="&eop;ARCHIVED">
   <dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;ACQUIRED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;CANCELLED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;FAILED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;PLANNED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;POTENTIAL">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;REJECTED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>
<eop:StatusType rdf:about="&eop;QUALITYDEGRADED">
		<dc:description>Acquisition status constant.</dc:description>
</eop:StatusType>


<owl:ObjectProperty rdf:about="&eop;status">
   <rdfs:label>status</rdfs:label>
   <rdfs:domain rdf:resource="&eop;Properties"/>
   
   <rdfs:range rdf:resource="&eop;StatusType"/>
	
   <dc:description>Refers to product status.
   
Fixed Values :
 - ARCHIVED
 - ACQUIRED
 - CANCELLED
 - FAILED
 - PLANNED
 - POTENTIAL
 - REJECTED
 - QUALITYDEGRADED.</dc:description>
   <rdf:type rdf:resource="&owl;FunctionalProperty"/>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&eop;acquisitionInformation">
   <rdfs:label>acquisitionInformation</rdfs:label>
   <rdfs:domain rdf:resource="&eop;Properties"/>
   <rdfs:range rdf:resource="&eop;AcquisitionInformation"/>
   <rdfs:comment>Contains acquisition information.</rdfs:comment>
   <dc:description>Contains acquisition information.</dc:description>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&eop;productInformation">
   <rdfs:label>productInformation</rdfs:label>
   <rdfs:domain rdf:resource="&eop;Properties"/>
   <rdfs:range rdf:resource="&eop;ProductInformation"/>
   <rdfs:comment>Contains product information.</rdfs:comment>
   <dc:description>Contains product information.</dc:description>
   <rdf:type rdf:resource="&owl;FunctionalProperty"/>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>



<!-- 
       Table 6: Links Class and properties 
-->

<owl:ObjectProperty rdf:about="&iana;describedby">
   <rdfs:label>qualityReport</rdfs:label>
   <dc:description>Reference to the location of a related quality report as defined by OGC 14-055r2 §7.1.2.</dc:description>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003, OGC 14-055r2</dcterms:source>
   <rdfs:seeAlso rdf:resource="https://tools.ietf.org/html/rfc5988"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&iana;related">
   <rdfs:label>related</rdfs:label>
   <dc:description>Reference to the location of a related resource, e.g. cloud mask, snow mask.</dc:description>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
   <rdfs:seeAlso rdf:resource="https://tools.ietf.org/html/rfc5988"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&iana;up">
   <rdfs:label>up</rdfs:label>
   <dc:description>Reference to the metadata of higher level resource (e.g.  Collection, also identified by parentIdentifier) resource.</dc:description>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
   <rdfs:seeAlso rdf:resource="https://tools.ietf.org/html/rfc5988"/>
</owl:ObjectProperty>

<!-- 
       Table 7: Atom Link Class and properties 
-->


<owl:Class rdf:about="&eop;CategoryType">
  <rdfs:label>CategoryType</rdfs:label>
  <rdfs:comment>Enumeration of category constants.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
</owl:Class>

<eop:CategoryType rdf:about="&eop;THUMBNAIL">
		       <dc:description>Category constant.</dc:description>
</eop:CategoryType>
<eop:CategoryType rdf:about="&eop;QUICKLOOK">
		       <dc:description>Category constant.</dc:description>
</eop:CategoryType>
<eop:CategoryType rdf:about="&eop;ALBUM">
		       <dc:description>Category constant.</dc:description>
</eop:CategoryType>
<eop:CategoryType rdf:about="&eop;SNOW">
		       <dc:description>Category constant.</dc:description>
</eop:CategoryType>
<eop:CategoryType rdf:about="&eop;CLOUD">
		       <dc:description>Category constant.</dc:description>
</eop:CategoryType>
<eop:CategoryType rdf:about="&eop;QUALITY">  
             <dc:description>Category constant.</dc:description>
</eop:CategoryType>

<owl:ObjectProperty rdf:about="&media;category">
   <rdfs:label>category</rdfs:label>
   <dc:description>Media category of the related resource which corresponds to a preview or mask:
- THUMBNAIL
- QUICKLOOK
- ALBUM
- SNOW
- CLOUD
- QUALITY
Only applicable for media resources.</dc:description>
   <rdfs:isDefinedBy rdf:resource="&media;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003, MediaRSS</dcterms:source>
   <rdfs:seeAlso rdf:resource="http://www.rssboard.org/media-rss#media-category"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="&media;expression">
   <rdfs:label>expression</rdfs:label>
   <dc:description>Determines if the related resource is a sample or the full version of the product.  Use "full" for “product/ProductInformation/”.  Use "sample" for "browse/BrowseInformation/" or "mask/MaskInformation/".</dc:description>
   <rdfs:isDefinedBy rdf:resource="&media;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003, MediaRSS</dcterms:source>
   <rdfs:seeAlso rdf:resource="http://www.rssboard.org/media-rss#media-content"/>
</owl:DatatypeProperty>


<!-- 
       Table 8: DataIdentification Class and properties 
-->

<owl:Class rdf:about="&eop;DataIdentification">
  <rdfs:label>DataIdentification</rdfs:label>
  <rdfs:comment>Base class of Properties.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource ="&eop;"/>
  <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:Class>

<owl:DatatypeProperty rdf:about="&eop;parentIdentifier">
   <rdfs:label>parentIdentifier</rdfs:label>
   <rdfs:comment>Collection identifier.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;DataIdentification"/>
   <vann:example>
{
    "parentIdentifier": "SEA_GEC_1P"
}
   </vann:example>
   <rdfs:range rdf:resource="&xsd;string"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;doi">
   <rdfs:label>doi</rdfs:label>
   <rdfs:comment>Digital Object Identifier identifying the product (see http://www.doi.org).</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;DataIdentification"/>
   <rdfs:range rdf:resource="&xsd;string"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:ObjectProperty rdf:about="&eop;additionalAttributes">
   <rdfs:label>additionalAttributes</rdfs:label>
   <rdfs:comment>Container for ad-hoc additional metadata attributes which are organisation or mission specific.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;DataIdentification"/>
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>


<!-- 
       Table 10: VerticalSpatialDomain Class and properties 
-->

<owl:Class rdf:about="&eop;VerticalSpatialDomain">
  <rdfs:label>VerticalSpatialDomain</rdfs:label>
  <rdfs:comment>Contains the properties related to the spatial extent in the vertical dimension.  Its properties are inherited by the AcquisitionParameters object.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource ="&eop;"/>
  <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:Class>

<owl:Class rdf:about="&eop;Unit">
  <rdfs:label>Unit</rdfs:label>
  <rdfs:comment>Base class to include all unit constants.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
</owl:Class>

<owl:Class rdf:about="&eop;AltitudeUnitType">
  <rdfs:label>AltitudeUnitType</rdfs:label>
  <rdfs:comment>Enumeration of altitudeUnit constants.</rdfs:comment>
  <owl:equivalentClass>
      <owl:Class>
        <owl:oneOf rdf:parseType="Collection">
          <rdf:Description rdf:about="&eop;Unit/Bar"/>
          <rdf:Description rdf:about="&eop;Unit/Meter"/>
        </owl:oneOf>
      </owl:Class>
    </owl:equivalentClass>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
</owl:Class>

<owl:NamedIndividual rdf:about="&eop;Unit/Meter">
    <rdfs:label>meter</rdfs:label>
    <dc:description>Unit constant representing SI unit "meter" (m).</dc:description>
    <rdf:type rdf:resource="&eop;Unit"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="&eop;Unit/Bar">
    <rdfs:label>bar</rdfs:label>
    <dc:description>Unit constant representing non-SI unit "bar" (bar).</dc:description>
    <rdf:type rdf:resource="&eop;Unit"/>
</owl:NamedIndividual>

<owl:ObjectProperty rdf:about="&eop;altitudeUnit">
   <rdfs:label>locationUnit</rdfs:label>
   <rdfs:comment>Unit of measure used to express lowestLocation and highestLocation: meter (m) or bar (bar). Default is meter (m).</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;VerticalSpatialDomain"/>
   <rdfs:range rdf:resource="&eop;AltitudeUnitType"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="&eop;minimumAltitude">
   <rdfs:label>lowestLocation</rdfs:label>
   <rdfs:comment>Upper bound of measurements in vertical dimension.  Unit of measure defined by locationUnit property.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;VerticalSpatialDomain"/>
   <rdfs:range rdf:resource="&xsd;double"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;maximumAltitude">
   <rdfs:label>highestLocation</rdfs:label>
   <rdfs:comment>Lower bound of measurements in vertical dimension.   Unit of measure defined by locationUnit property.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;VerticalSpatialDomain"/>
   <rdfs:range rdf:resource="&xsd;double"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<!-- 
       Table 11: OrbitParameters Class and properties 
-->

<owl:Class rdf:about="&eop;OrbitParameters">
  <rdfs:label>OrbitParameters</rdfs:label>
  <rdfs:comment>Contains the properties related to the orbit.  Its properties are inherited by the AcquisitionParameters object.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource ="&eop;"/>
  <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:Class>

<owl:Class rdf:about="&eop;OrbitDirectionType">
  <rdfs:label>OrbitDirectionType</rdfs:label>
  <rdfs:comment>Enumeration of orbit direction constants.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
</owl:Class>

<eop:OrbitDirectionType rdf:about="&eop;ASCENDING">
   <dc:description>Orbit direction constant.</dc:description>
</eop:OrbitDirectionType>

<eop:OrbitDirectionType rdf:about="&eop;DESCENDING">
   <dc:description>Orbit direction constant.</dc:description>
</eop:OrbitDirectionType>

<owl:ObjectProperty rdf:about="&eop;orbitDirection">
   <rdfs:label>orbitDirection</rdfs:label>
   <rdfs:comment>Acquisition orbit direction at the start of the acquisition/product.
Values:
•	ASCENDING
•	DESCENDING</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/> 
   <rdfs:range rdf:resource="&eop;OrbitDirectionType"/>
    
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&eop;lastOrbitDirection">
   <rdfs:label>lastOrbitDirection</rdfs:label>
   <rdfs:comment>Acquisition orbit direction at the end of the acquisition/product. Assumed to be identical to orbitDirection if not present.
Values:
•	ASCENDING
•	DESCENDING</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/> 
   <rdfs:range rdf:resource="&eop;OrbitDirectionType"/>
    
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="&eop;orbitDuration">
   <rdfs:label>orbitDuration</rdfs:label>
   <rdfs:comment>Actual orbit duration in milliseconds.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/>
   <rdfs:range rdf:resource="&xsd;integer"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;orbitNumber">
   <rdfs:label>orbitNumber</rdfs:label>
   <rdfs:comment>Acquisition orbit number.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/>
   <rdfs:range rdf:resource="&xsd;integer"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;lastOrbitNumber">
   <rdfs:label>lastOrbitNumber</rdfs:label>
   <rdfs:comment>Acquisition last orbit number.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/>
   <rdfs:range rdf:resource="&xsd;integer"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;ascendingNodeDate">
   <rdfs:label>ascendingNodeDate</rdfs:label>
   <rdfs:comment>UTC date and time at ascending node of orbit.</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/>
   <rdfs:range rdf:resource="&xsd;dateTime"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="&eop;ascendingNodeLongitude">
   <rdfs:label>ascendingNodeLongitude</rdfs:label>
   <rdfs:comment>Longitude at ascending node of orbit. Should be expressed in degrees.
</rdfs:comment>
   <rdfs:domain rdf:resource="&eop;OrbitParameters"/>
   <rdfs:range rdf:resource="&xsd;double"/>  
   <rdfs:isDefinedBy rdf:resource="&eop;"/>
   <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:DatatypeProperty>


<!-- 
       Table 12: TemporalInformation Class and properties 
-->

<owl:Class rdf:about="&eop;TemporalInformation">
  <rdfs:label>TemporalInformation</rdfs:label>
  <rdfs:comment>Contains the properties related to the start and end time of the acquisition of the data.  Its properties are inherited by the AcquisitionParameters object.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource ="&eop;"/>
  <dcterms:source rdf:datatype="&xsd;string">OGC 17-003</dcterms:source>
</owl:Class>


<!-- 
       Table 13: AcquisitionInformation Class and properties 
-->

<owl:Class rdf:about="&eop;AcquisitionInformation">
  <rdfs:label>AcquisitionInformation</rdfs:label>
  <rdfs:comment>Acquisition information of the Earth Observation as defined in OGC 17-003</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource="&eop;"/>
  <vann:example>
{
    "@context": "https://www.opengis.net/eo-geojson/1.0",
